.page-not-found
  height 200px
  width 200px
  margin 30px auto
  border-radius 100px
  text-align center
  filter drop-shadow(1px 5px 5px var(--primary-color))

.page-not-found-content
  display flex
  flex-direction column
  align-items center
  color var(--primary-color)

  span 
    margin 10px 0px

    p
      display inline-block
      font-weight bold
      padding 0px 0px 0px 4px
      cursor pointer