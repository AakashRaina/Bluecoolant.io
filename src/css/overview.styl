@import './theme.styl';

.overview
  padding 10px 0px 10px 10px

  .title
    color var(--primary-color)
    font-size $secondary-font
    font-weight bold

    span, a
      color var(--primary-color)
      position relative

  .date, .description
    color var(--secondary-color)
  
  .project-url
    font-size 13px
    color var(--secondary-color)
