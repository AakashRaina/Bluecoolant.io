@import './theme.styl'

.skills
  color var(--primary-color)
  padding 10px 0px 10px 10px

  select{
    padding 10px
    font-size $secondary-font
    font-weight bold
    color var(--secondary-color)
    border-top 0px 
    border-right 0px 
    border-bottom 1px solid #e1e1e1
    border-left 0px
    background-color var(--bg)
  }